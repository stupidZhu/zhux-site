{
  "private": true,
  "name": "dumi-app",
  "scripts": {
    "start": "dumi dev",
    "build": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\""
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/react-transition-group": "^4.4.5",
    "@umijs/plugin-sass": "^1.1.1",
    "dumi": "^1.0.13",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "ahooks": "^3.7.0",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.4",
    "lodash": "^4.17.21",
    "react-transition-group": "^4.4.5",
    "zhux-utils": "^0.1.18",
    "zhux-utils-react": "^0.0.3"
  }
}
